<div class="modal fade" id="modalIconSelector" tabindex="-1" role="dialog">
	<div class="modal-dialog modal-dialog-centered modal-dialog-scrollable float-right">
		<div class="modal-content shadow bg-secondary h-100">
			<div class="modal-header">
				<h5 class="modal-title">Sélection de l'icône</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<div class="row">
					{% for icon in iconList %}
						<span class="iconify icon-select col-3 px-0 my-3" width="35" data-icon="{{icon}}"></span>
					{% endfor %}
				</div>
			</div>
		</div>
	</div>
</div>
