<div class="modal fade" id="modalUploadForm" tabindex="-1" role="dialog" aria-labelledby="modalUploadForm" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-xl">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title">Uploader un media</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				{{form_start(uploadForm)}}
				{{form_widget(uploadForm)}}
				<div class="progress d-none my-3">
					<div class="progress-bar" role="progressbar"></div>
				</div>
				<button type="submit"  class="btn btn-success">Envoyer</button>
				<button class="btn btn-darker" type="button" data-toggle="modal" data-target="#modalUploadForm" aria-expanded="false" aria-controls="collapseExample">Annuler</button>
				{{form_end(uploadForm)}}
			</div>
		</div>
	</div>
</div>
